<Window x:Class="Template4432._4432_Sharipov"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4432"
        mc:Ignorable="d"
        Title="_4432_Sharipov" Height="450" Width="800"
        Background="Red">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Foreground="Green" TextAlignment="Center" VerticalAlignment="Center" FontSize="50">Шарипов Булат 13 лет</TextBlock>
        <StackPanel Grid.Row="1">
            <Button x:Name="btnImport" Height="30" Width="150" Click="btnImport_Click">Импорт данных</Button>
            <Button x:Name="btnExport" Height="30" Width="150" Click="btnExport_Click">Экспорт в Excel</Button>
        </StackPanel>
    </Grid>
</Window>
